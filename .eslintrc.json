{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/extensions": ["error", "never", { "js": "always", "json": "always" }],
        "indent": "off",
        "no-console": "off",
        "no-undef": "off",
        "no-unused-vars": "off",
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}